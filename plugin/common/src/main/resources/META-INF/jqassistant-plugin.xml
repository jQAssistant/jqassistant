<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/analysis/plugin/schema/v1.0">
    <description>Provides common elements to be re-used by other plugins.</description>
    <store>
        <type>com.buschmais.jqassistant.plugin.common.impl.store.descriptor.ArtifactDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.common.impl.store.descriptor.DependsOnDescriptor</type>
    </store>
    <scanner>
        <plugin>com.buschmais.jqassistant.plugin.common.impl.scanner.DirectoryScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.common.impl.scanner.FileScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.common.impl.scanner.IterableScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.common.impl.scanner.UrlScannerPlugin</plugin>
    </scanner>
</jqa-plugin:jqassistant-plugin>