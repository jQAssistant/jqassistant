<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://schema.jqassistant.org/plugin/jqassistant-plugin-v2.4.xsd"
                    id="jqa.plugin.xml"
                    name="jQAssistant XML Plugin"
                    version="${project.version}">
    <description>Provides scanners for XML and XSD documents.</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlFileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlNamespaceDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlElementDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlAttributeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlTextDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.xml.api.model.XmlCDataDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.xml.impl.scanner.XmlSourceScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.xml.impl.scanner.XmlFileScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.xml.impl.scanner.XsdFileScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
