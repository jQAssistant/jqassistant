<jpd:jqassistant-plugin xmlns:jpd="http://schema.jqassistant.org/plugin/v1.10"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.10 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.10.xsd"
                        name="JSON" id="jqa.plugin.json">
    <description>Provides a scanner for JSON files</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONArrayDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONFileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONKeyDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONObjectDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.json.api.model.JSONScalarValueDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.json.impl.scanner.JSONFileScannerPlugin</class>
    </scanner>
</jpd:jqassistant-plugin>
