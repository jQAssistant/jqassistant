<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/analysis/plugin/schema/v1.0">
    <description>Provides scanner for Java elements (e.g. packages, classes, manifest and property files) and rules for common language concepts (e.g. Deprecation), dependencies and metrics.
    </description>
    <store>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.PackageDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ClassTypeDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.InterfaceTypeDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.EnumTypeDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.AnnotationTypeDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.FieldDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.MethodDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.InvokesDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ReadsDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.WritesDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ConstructorDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ParameterDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ArrayValueDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ClassValueDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.PropertyDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.AnnotationValueDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.PrimitiveValueDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.EnumValueDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.PropertyFileDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ManifestFileDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ManifestSectionDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ManifestEntryDescriptor</type>
        <type>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ServiceLoaderDescriptor</type>
    </store>
    <scanner>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.PackageScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.ClassScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.PropertyFileScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.ManifestFileScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.ServiceLoaderScannerPlugin</plugin>
    </scanner>
    <rules>
        <resources>
            <resource>dependency.xml</resource>
            <resource>java.xml</resource>
            <resource>package.xml</resource>
            <resource>metric.xml</resource>
        </resources>
    </rules>
</jqa-plugin:jqassistant-plugin>