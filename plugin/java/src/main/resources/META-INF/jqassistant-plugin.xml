<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/analysis/plugin/schema/v1.0">
    <store>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.PackageDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.TypeDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.FieldDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.MethodDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.ParameterDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.ValueDescriptorMapper</mapper>
        <mapper>com.buschmais.jqassistant.plugin.java.impl.store.mapper.PropertyFileDescriptorMapper</mapper>
    </store>
    <scanner>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.PackageScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.ClassScannerPlugin</plugin>
        <plugin>com.buschmais.jqassistant.plugin.java.impl.scanner.PropertyFileScannerPlugin</plugin>
    </scanner>
    <rules>
        <resources>
            <resource>dependency.xml</resource>
            <resource>java.xml</resource>
            <resource>package.xml</resource>
        </resources>
    </rules>
</jqa-plugin:jqassistant-plugin>