        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.AnnotationTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.FieldDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.MethodDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.InvokesDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ReadsDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.WritesDescriptor</class>
