        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.EnumValueDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.PropertyFileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.java.impl.store.descriptor.ManifestFileDescriptor</class>
