<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0">
    <description>Provides a scanner for Maven 3 modules.</description>
    <types>
        <class>com.buschmais.jqassistant.plugin.maven3.impl.scanner.impl.store.MavenDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.maven3.impl.scanner.impl.store.MavenProjectDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.maven3.impl.scanner.impl.store.MavenProjectDirectoryDescriptor</class>
    </types>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.maven3.impl.scanner.impl.scanner.MavenProjectMavenScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>