<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4"
                               name="YAML">
    <description>Provides yet another a scanner for YAML files</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.yaml2.api.model.YAML2Descriptor</class>
        <class>com.buschmais.jqassistant.plugin.yaml2.api.model.YAML2DocumentDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.yaml2.api.model.YAML2FileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.yaml2.api.model.YAML2SequenceDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.yaml2.impl.scanner.YAML2FileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
