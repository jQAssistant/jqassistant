<?xml version="1.0" encoding="UTF-8"?>
<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/analysis/rules/schema/v1.0">

    <group id="default">
        <includeConcept refId="encoding"/>
        <includeConstraint refId="java:AvoidCyclicPackageDependencies"/>
        <includeConstraint refId="java:AvoidCyclicArtifactDependencies"/>
    </group>

    <concept id="encoding">
        <description>This description contains german umlauts: ÄÖÜß</description>
        <cypher>
            RETURN 'ÄÖÜß' as umlauts
        </cypher>
    </concept>
</jqa:jqassistant-rules>
