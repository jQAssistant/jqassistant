<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/rule/schema/v1.4">

    <group id="default">
        <includeConcept refId="test:*"/>
    </group>

    <concept id="test:APOC" >
        <description>List APOC functions</description>
        <cypher>
            call apoc.help('search')
        </cypher>
    </concept>

    <concept id="test:GraphAlgorithms" >
        <description>List Graph Algorithms</description>
        <cypher>
            call algo.list()
        </cypher>
    </concept>

</jqa:jqassistant-rules>
