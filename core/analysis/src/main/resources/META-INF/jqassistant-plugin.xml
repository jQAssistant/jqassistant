<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4" name="Core Analysis">
    <description>Provides elements for storing analysis results.</description>
    <model>
        <class>com.buschmais.jqassistant.core.analysis.api.model.ConceptDescriptor</class>
    </model>
    <ruleSourceReader>
        <class>com.buschmais.jqassistant.core.rule.impl.reader.XmlRuleSourceReaderPlugin</class>
        <class>com.buschmais.jqassistant.core.rule.impl.reader.AsciiDocRuleSourceReaderPlugin</class>
    </ruleSourceReader>
    <ruleLanguage>
        <class>com.buschmais.jqassistant.core.analysis.impl.CypherLanguagePlugin</class>
        <class>com.buschmais.jqassistant.core.analysis.impl.ScriptLanguagePlugin</class>
    </ruleLanguage>
</jqa-plugin:jqassistant-plugin>
