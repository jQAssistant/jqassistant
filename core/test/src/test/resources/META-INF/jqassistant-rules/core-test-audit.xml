<jqassistant-rules xmlns="http://schema.jqassistant.org/rule/v2.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://schema.jqassistant.org/rule/v2.9 http://schema.jqassistant.org/rule/jqassistant-rule-v2.9.xsd">

    <group id="core-test-audit:Group">
        <includeConstraint refId="core-test-audit:Constraint"/>
    </group>

    <constraint id="core-test-audit:Constraint">
        <description>Test Concept</description>
        <cypher><![CDATA[
        MATCH
          (n:TestLabel)
        RETURN
          n
        ]]></cypher>
    </constraint>

    <constraint id="core-test-audit:OverridingConstraint">
        <overridesConstraint refId="core-test-audit:Constraint"/>
        <requiresConcept refId="core-test-audit:Concept"/>
        <description>Test Concept</description>
        <cypher><![CDATA[
        MATCH
          (n:TestLabel)
        RETURN
          n
        ]]></cypher>
    </constraint>

    <concept id="core-test-audit:Concept">
        <description>Test Concept</description>
        <cypher><![CDATA[
        MATCH
          (n:TestLabel)
        RETURN
          n
        ]]></cypher>
    </concept>

    <concept id="core-test-audit:OverridingConcept">
        <overridesConcept refId="core-test-audit:Concept" />
        <description>Test Concept</description>
        <cypher><![CDATA[
        MATCH
          (n:TestLabel)
        RETURN
          n
        ]]></cypher>
    </concept>

    <concept id="core-test-audit:ProvidingConcept">
        <providesConcept refId="core-test-audit:OverridingConcept"/>
        <description>Test Concept</description>
        <cypher><![CDATA[
        MERGE
          (n:TestLabel)
        RETURN
          n
        ]]></cypher>
    </concept>

</jqassistant-rules>
