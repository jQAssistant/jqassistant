<jqassistant-rules xmlns="http://schema.jqassistant.org/rule/v1.10"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://schema.jqassistant.org/rule/v1.10 http://schema.jqassistant.org/rule/jqassistant-rule-v1.10.xsd">

    <concept id="test:CypherConcept">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <cypher><![CDATA[
            Cypher Concept
        ]]></cypher>
    </concept>

    <concept id="test:ScriptConcept">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <script language="javascript"><![CDATA[
            Script Concept
        ]]></script>
    </concept>

    <concept id="test:SourceConcept">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <source language="cypher"><![CDATA[
          Source Concept
        ]]></source>
    </concept>

    <concept id="test:SourceConceptUpperCase">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <source language="CYPHER"><![CDATA[
          Source Concept (upper case)
        ]]></source>
    </concept>


    <constraint id="test:CypherConstraint">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <cypher><![CDATA[
            Cypher Constraint
        ]]></cypher>
    </constraint>

    <constraint id="test:ScriptConstraint">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <script language="javascript"><![CDATA[
            Script Constraint
        ]]></script>
    </constraint>

    <constraint id="test:SourceConstraint">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <source language="cypher"><![CDATA[
            Source Constraint
        ]]></source>
    </constraint>

    <constraint id="test:SourceConstraintUpperCase">
        <requiresConcept refId="test:RequiredConcept"/>
        <description></description>
        <source language="CYPHER"><![CDATA[
            Source Constraint (upper case)
        ]]></source>
    </constraint>

</jqassistant-rules>
