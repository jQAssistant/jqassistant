<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/analysis/rules/schema/v1.1">

    <concept id="test:Concept">
        <description>Test Concept.</description>
        <cypher><![CDATA[
            match
              (n)
            where
              n.name = {name}
            return
              n.property1 as Property1, n.property2 as Property2
            limit
              {limit}
        ]]></cypher>
        <report type="custom" primaryColumn="n" keyColumns="Property1,Property2">
            <property name="key1">value1</property>
            <property name="key2">value2</property>
        </report>
    </concept>

    <constraint id="test:Constraint">
        <description>Test Constraint.</description>
        <cypher><![CDATA[
            match
              (n)
            where
              n.name = {name}
            return
              n.property1 as Property1, n.property2 as Property2
            limit
              {limit}
        ]]></cypher>
        <report type="custom" primaryColumn="n" keyColumns="Property1 , Property2">
            <property name="key1">value1</property>
            <property name="key2">value2</property>
        </report>
    </constraint>
</jqa:jqassistant-rules>
