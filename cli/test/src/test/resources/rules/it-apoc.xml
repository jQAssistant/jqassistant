<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/analysis/rules/schema/v1.2">

    <concept id="it-apoc:APOCHelp">
        <description>Calls APOC Help</description>
        <cypher>
            call apoc.help('apoc')
        </cypher>
    </concept>

    <concept id="it-apoc:APOCMetadata">
        <description>Calls APOC Metadata (restricted procedure, requires according setting for embedded server)</description>
        <cypher>
            call apoc.meta.data()
        </cypher>
    </concept>
</jqa:jqassistant-rules>
